<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MarqueeTextView">
        <attr name="scroll_first_delay" format="integer"/>
        <attr name="scroll_speed" format="integer" />
        <attr name="scroll_times" format="integer"/>
        <attr name="scroll_start_location">
            <!-- start scroll from left -->
            <enum name="start_left" value="0"/>
            <!-- start scroll from right -->
            <enum name="start_right" value="1"/>
        </attr>
    </declare-styleable>
</resources>